{% block block_image_text_six_block %}
    {% set columns = 6 %}
    {% set id = element.id %}

<style>
.block-image-text-six-block {
    display: flex;
    flex-wrap: wrap;
    justify-content: center;
    align-content: stretch;
    align-items: start;
    grid-gap: 10px;
    width:100%;
}

.block-image-text-six-block .cms-element-text {
    padding: 10px;
}

.block-image-text-six-block .cms-image-container.is-cover {
    min-height: 180px !important
}

.block-image-text-six-block .block-image-text-six-block__five,
.block-image-text-six-block .block-image-text-six-block__four,
.block-image-text-six-block .block-image-text-six-block__one,
.block-image-text-six-block .block-image-text-six-block__six,
.block-image-text-six-block .block-image-text-six-block__three,
.block-image-text-six-block .block-image-text-six-block__two {
    min-width: 200px;
    flex-grow: 1;
    {# box-shadow: 0 0 4px 0 rgba(0, 0, 0, .2) #}
}

.block-image-text-six-block a {
   background-color: rgba(0, 0, 0, 0);
   padding: 0px;
}
</style>




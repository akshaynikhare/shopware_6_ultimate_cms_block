    {% block block_image_text_six_block__one %}
        
            <div class="block-image-text-six-block__one">
                {% block block_image_text_six_block__one_image %}
                    {% set element = block.slots.getSlot('one-image') %}

                    <div data-cms-element-id="{{ id }}">
                        {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
                    </div>
                {% endblock %}

                {% block block_image_text_six_block__one_text %}
